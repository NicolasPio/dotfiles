    "vimderbar.enabled": false,
    "dnbard.documents-toolbar.showCloseButton": true,
    "brackets-ruler.rulerEnabled": true,
    "brackets-ruler.guideEnabled": false,
    "themes.theme": "josephgf.obsidian2",
    "markdown-preview.syncScroll": true,
    "brackets-git.gitTimeout": 30
}
